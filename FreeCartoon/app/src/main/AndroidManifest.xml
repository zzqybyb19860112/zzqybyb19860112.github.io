<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ybyb.zzq.freecartoon">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.GuideActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ui.cartoon.young.YoungCartoonActivity"/>
        <activity android:name=".ui.cartoon.child.ChildCartoonActivity"/>
        <activity android:name=".ui.cartoon.girl.GirlCartoonActivity"/>
        <activity android:name=".ui.cartoon.usa.USACartoonActivity"/>
        <activity android:name=".ui.look.ChapterActivity"/>
        <activity android:name=".ui.detail.CartoonActivity"/>
        <activity android:name=".ui.main.MainActivity"/>
    </application>

</manifest>